<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vjay.libararymanagement">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BarcodeReaderActivity"/>
        <activity android:name=".BookAvailedByUserActivity" />
        <activity android:name=".GetNewBookActivity" />
        <activity android:name=".UpdateBookReturnActivity" />
        <activity android:name=".BookDetailActivity" />
        <activity android:name=".BooksAvailableActivity" />
        <activity android:name=".AddBookActivity" />
        <activity android:name=".DashBoardActivity" />
        <activity android:name=".ResgiterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>